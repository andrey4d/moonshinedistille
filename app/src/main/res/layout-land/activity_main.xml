<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.danilovav.moonshinedistiller.MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <EditText
        android:id="@+id/edtBody"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="70"
        android:inputType="numberDecimal"
        android:text="70"
        app:layout_constraintLeft_toLeftOf="@+id/edtAfterHeads"
        app:layout_constraintTop_toBottomOf="@+id/edtAfterHeads" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnAfterHeads"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnAfterHeads"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvBodyOut"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/btnAfterHeads"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnHeads"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnHeads"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvAfterHeadsOut" />

    <TextView
        android:id="@+id/tvAlc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="@string/strAclRAW"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInitCube" />

    <EditText
        android:id="@+id/edtAlc"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Alc %"
        android:inputType="numberDecimal"
        android:text="100"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvAlc"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvAlcV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:text="@string/strRAW_V"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtAlc"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/edtAlcV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="3000"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvAlcV"
        app:layout_constraintLeft_toLeftOf="@+id/edtAlc"
        app:layout_constraintRight_toRightOf="@+id/edtAlc"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvWaterV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:text="@string/strWater_V"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtAlcV"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/edtWaterV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="3000"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvWaterV"
        app:layout_constraintLeft_toLeftOf="@+id/edtAlcV"
        app:layout_constraintRight_toRightOf="@+id/edtAlcV"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvInitCube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/strInitCube"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.08" />

    <TextView
        android:id="@+id/tvAlcInCube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/strAlcInCube"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtWaterV"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tvVolAbsAlc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/strValAbsAlc"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvAlcInCube"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tvAlcInCubeOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:text="0"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvAlcInCube"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/tvAlcInCube"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/tvCurSet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/strTitleFractions"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.702"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCalcCube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickCalculate"
        android:text="@string/strCalculateCube"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="131dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvVolAbsAlc"
        app:layout_constraintRight_toLeftOf="@+id/view2"
        android:layout_marginRight="1dp" />

    <TextView
        android:id="@+id/tvHeads"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/strHeads"
        android:layout_marginStart="11dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edtHeads"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/edtHeads"
        android:layout_marginLeft="11dp" />

    <TextView
        android:id="@+id/tvAfterHeads"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="@string/strAfterHeads"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/edtAfterHeads"
        android:layout_marginLeft="11dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/edtAfterHeads" />

    <TextView
        android:id="@+id/tvBody"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="@string/strBody"
        app:layout_constraintBaseline_toBaselineOf="@+id/edtBody"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/edtBody"
        android:layout_marginLeft="11dp" />

    <TextView
        android:id="@+id/tvHeadsOut"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvHeads"
        app:layout_constraintHorizontal_bias="0.771" />

    <TextView
        android:id="@+id/tvAfterHeadsOut"
        android:layout_width="40dp"
        android:layout_height="17dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvAfterHeads"
        app:layout_constraintHorizontal_bias="0.771" />

    <TextView
        android:id="@+id/tvBodyOut"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvBody"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.771" />

    <TextView
        android:id="@+id/tvVolAbsAlcOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvVolAbsAlc"
        app:layout_constraintLeft_toLeftOf="@+id/tvAlcInCubeOut"
        tools:layout_constraintLeft_creator="1" />

    <EditText
        android:id="@+id/edtHeads"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="5"
        android:inputType="numberDecimal"
        android:text="5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvCurSet"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/btnHeads"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="6dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="6dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvHeadsOut" />

    <EditText
        android:id="@+id/edtAfterHeads"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="5"
        android:inputType="numberDecimal"
        android:text="5"
        app:layout_constraintLeft_toLeftOf="@+id/edtHeads"
        app:layout_constraintTop_toBottomOf="@+id/edtHeads" />

    <View
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:layout_marginRight="8dp"
        android:background="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toRightOf="@+id/edtWaterV"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:id="@+id/view2" />
</android.support.constraint.ConstraintLayout>
